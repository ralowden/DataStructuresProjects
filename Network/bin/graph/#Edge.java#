package graph;

public class Edge {
	private int a;
	private int b;
	private double slope = (double) a / (double) b;
	
	public Edge(int a, int b) {
		this.a = a;
		this.b = b;
		slope = 0;
	}
	
	public int getA() {
		return a;
	}
	public int getB() {
		return b;
	}
	
	public double getSlope() {
		return slope;
	}
}
